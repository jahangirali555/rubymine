<%= form_for @login, url: dashboard_update_path(:id => @login.id), method: :put do |f|  %>

 
  <div class="form-group"><%= f.label :email %><br />
  <%=  @login.email %></div>
   <div class="form-group">
    <%= f.label :usertype_id %><br>
    <%= f.select :usertype_id, options_for_select([["Select One", ""], ["admin","2"],["user", "3"]]), class: "form-control" %>
  </div>
  

<div class="form-group"><%= f.submit "Update" , class: "btn btn-primary"%></div>


<% end %>

<%= link_to "Back", :back %>
